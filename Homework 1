#include <stdio.h>

int main(void)
{
    int grade;
    printf("Please enter your grade:");
    scanf("&d", &grade);
    
    if (grade < 0)
    {
              printf("Your grade can't be negative.");
              }
    else if (grade > 0 && grade < 45)
    {
         printf("Your grade is F.");
         }
    else if (grade >= 45 && grade <= 54)
    {
         printf("Your grade is D.");
         }
    else if (grade > 54 && grade < 75)
    {
         printf("Your grade is C.");
         }
    else if (grade >= 75 && grade < 90)
    {
         printf("Your grade is B.");
         }
    else if (grade >= 90 && grade <= 100)
    {
         printf("Your grade is A.");
         }
    else (grade > 100)
    {
         
         printf("Your grade can't be grater than 100, please enter your grade correctly");
         }
         
    return 0;
    
}
